<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout continueOnError="false" enabled="true" name="SC-Priority2Callout">
  <DisplayName>SC-Priority2Callout</DisplayName>
  <Properties/>
  <Request clearPayload="true" variable="myRequest">
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <Set>
      <Headers>
        <Header name="apikey">{priority2APIKey}</Header>
      </Headers>
    </Set>
  </Request>
  <Response>response</Response>
  <HTTPTargetConnection>
    <Properties/>
    <URL>https://{priority2Host}/{priority2Path}</URL>
  </HTTPTargetConnection>
</ServiceCallout>
